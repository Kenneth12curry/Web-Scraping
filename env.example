# Configuration FinData IA-M.K
# Copiez ce fichier vers .env et remplissez vos valeurs

# API Keys
SCRAPEDO_API_KEY=your_scrapedo_token_here
GROQ_API_KEY=your_groq_token_here

# Sécurité
JWT_SECRET_KEY=your_jwt_secret_key_here_change_this_in_production

# Identifiants Admin (CHANGEZ EN PRODUCTION !)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Configuration serveur
FLASK_ENV=development
FLASK_DEBUG=true
PORT=8080

# Base de données MySQL
MYSQL_HOST=localhost
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password_here
MYSQL_DATABASE=findata

# Cache Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Configuration CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:8080

# Monitoring et Observabilité
SENTRY_DSN=your_sentry_dsn_here
PROMETHEUS_ENABLED=true
LOG_LEVEL=INFO

# Configuration Docker
DOCKER_REGISTRY=ghcr.io
DOCKER_IMAGE_NAME=findata-ia-mk 